<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C3C0AB"
    tools:context=".MainActivity3">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_extrabold"
        android:text="Fantasy QFL"
        android:textSize="34dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.149" />

    <Button
        android:id="@+id/myteam"
        android:layout_width="202dp"
        android:layout_height="67dp"
        android:text="MY TEAM"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.111" />

    <Button
        android:id="@+id/exit"
        android:layout_width="202dp"
        android:layout_height="67dp"
        android:layout_marginTop="68dp"
        android:text="EXIT"
        app:layout_constraintEnd_toEndOf="@+id/settings"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/settings"
        app:layout_constraintTop_toBottomOf="@+id/settings" />

    <Button
        android:id="@+id/settings"
        android:layout_width="202dp"
        android:layout_height="67dp"
        android:layout_marginTop="69dp"
        android:text="SETTINGS"
        app:layout_constraintEnd_toEndOf="@+id/transfers"
        app:layout_constraintStart_toStartOf="@+id/transfers"
        app:layout_constraintTop_toBottomOf="@+id/transfers" />

    <Button
        android:id="@+id/transfers"
        android:layout_width="202dp"
        android:layout_height="67dp"
        android:layout_marginTop="72dp"
        android:text="TRANSFERS"
        app:layout_constraintEnd_toEndOf="@+id/myteam"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/myteam"
        app:layout_constraintTop_toBottomOf="@+id/myteam" />
</androidx.constraintlayout.widget.ConstraintLayout>